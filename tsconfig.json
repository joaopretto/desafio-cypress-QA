{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2016", "dom"],
    "types": ["cypress", "node"],
    
    "module": "esnext",
    "moduleResolution": "bundler",
    
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["**/*.ts"]
}